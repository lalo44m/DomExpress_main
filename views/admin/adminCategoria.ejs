<%- include("../partials/header") %>

<title>Administrador Inicio</title>
</head>
<body>
<%- include("../partials/navbar") %> 
<div class="container-fluid my-2">
    <%- include("../partials/flash") %>
  </div> 
<main>
    <div class="container my-3">
        <h2 class="text-center">Categoria de Pagina</h2>
        <hr>
        <h4>Todos los Productos</h4>
        <hr>
        <%if(data.length<1){%>
            <h3 class="text-center my-3">No hay categorias </h3>
        <%}else{%>
        <div class="products-list">
            
            <ul class="list-group">
            <% for(item of data.reverse()){ %> 
                <li class="list-group-item">
                    <div class="productList d-flex row">
                        <div class="itemProd col d-flex align-items-center">
                            <div class="prodInfo p-2">
                                <h5 class="name"><%=item.categorias%></h5>
                            </div>
                        </div>

                    </div>
                    </li>
            <% } %>
            </ul>
        </div>
   <% }%>
    </div>
</main>
<%- include("../partials/footer") %>